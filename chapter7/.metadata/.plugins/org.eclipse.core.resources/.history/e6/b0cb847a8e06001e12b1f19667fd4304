public class Chomework{
	public static void main(String[] args) {
		 A01 a =new A01();
		 double[] arr = {1.4, 4.6, 8.9};
		 System.out.println("arr的最大值=" + a.max(arr));
	}
	}
class A01 {
	public double max (double[]arr) {
		double max = arr[0];
		for (int i = 1; i <= arr.length; i++) {
			if(max < arr[i]) {
				max = arr[i];
			}
		}
		return max;
	}
}